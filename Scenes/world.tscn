[sub_resource type="GDScript" id="GDScript_3kg8w"]
script/source = "extends CharacterBody2D


@export var item: String = \"\"
@export var is_trash: bool

var is_interactable: bool = false
@onready var tooltip: Label = $Tooltip
@onready var animated_sprite_2d: AnimatedSprite2D = $AnimatedSprite2D

@onready var player: CharacterBody2D = $\"/root/World/Player\"


func _ready() -> void:
	tooltip.visible = false
	tooltip.text = \"Press SPACE to throw the material\" if is_trash else \"Press SPACE to grab the material\"
	animated_sprite_2d.play(\"trash\" if is_trash else \"chest\")

func _process(_delta: float) -> void:
	if is_interactable and Input.is_action_just_pressed(\"ui_select\"):
		if is_trash or player.check_item() == '':
			player.get_item(item)
		else:
			tooltip.text = \"You have your hands full right now\"
			tooltip.visible = true
			return
				

func _on_interaction_area_body_entered(_body: Node2D) -> void:
	is_interactable = true
	tooltip.visible = true


func _on_interaction_area_body_exited(_body: Node2D) -> void:
	is_interactable = false
	tooltip.visible = false
"

